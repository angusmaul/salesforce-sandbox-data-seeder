{
  "name": "salesforce-sandbox-data-seeder-web",
  "version": "1.0.0",
  "description": "Web-based wizard for Salesforce sandbox data seeding with AI assistance",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS=\"--max-old-space-size=8192\" concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "NODE_OPTIONS=\"--max-old-space-size=8192\" nodemon server/demo-server.js",
    "client:dev": "NODE_OPTIONS=\"--max-old-space-size=8192\" next dev -p 3000 -H 0.0.0.0",
    "build": "npm run server:build && npm run client:build",
    "server:build": "NODE_OPTIONS=\"--max-old-space-size=8192\" tsc -p server/tsconfig.json",
    "client:build": "NODE_OPTIONS=\"--max-old-space-size=8192\" next build",
    "start": "npm run server:start",
    "server:start": "NODE_OPTIONS=\"--max-old-space-size=8192\" node dist/server/index.js",
    "client:start": "NODE_OPTIONS=\"--max-old-space-size=8192\" next start",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "@faker-js/faker": "^8.4.1",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@types/archiver": "^6.0.3",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.16",
    "chalk": "^4.1.2",
    "clsx": "^2.0.0",
    "compression": "^1.7.4",
    "conf": "^10.2.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "framer-motion": "^10.16.5",
    "helmet": "^7.1.0",
    "jsforce": "^3.10.0",
    "lucide-react": "^0.294.0",
    "next": "^14.0.4",
    "node-fetch": "^3.3.2",
    "ora": "^5.4.1",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "recharts": "^2.15.4",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "tailwindcss": "^3.4.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.4",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
